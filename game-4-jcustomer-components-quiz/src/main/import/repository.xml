<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <game-4-jcustomer-components-quiz j:dependencies="codemirror-editor default game-4-jcustomer-core jexperience"
                                        j:modulePriority="0"
                                        j:moduleType="module"
                                        j:title="Game for jCustomer Quiz Components Module"
                                        jcr:primaryType="jnt:module">
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder">
            <quizzes j:contributeTypes="game4nt:quiz"
                     j:editableInContribution="true"
                     jcr:mixinTypes="jmix:contributeMode"
                     jcr:primaryType="jnt:contentFolder">
               <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="quizzes"/>
               <j:translation_de jcr:language="de"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="quiz"/>
               <j:translation_it jcr:language="it"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="quiz"/>
               <j:translation_fr jcr:language="fr"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="quiz"/>
               <j:translation_es jcr:language="es"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="quiz"/>
               <j:translation_de_DE jcr:language="de_DE"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="quiz"/>
               <j:translation_pt jcr:language="pt"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="questionário"/>
            </quizzes>
         </contents>
         <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <game4quiz j:applyOn="game4nt:quiz"
                       j:defaultTemplate="true"
                       j:hiddenTemplate="false"
                       jcr:primaryType="jnt:contentTemplate">
               <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Game4Quiz"/>
               <j:translation_fr jcr:language="fr"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="quiz"/>
               <j:translation_es jcr:language="es"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="quiz"/>
               <j:translation_de_DE jcr:language="de_DE"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="quiz"/>
               <j:translation_it jcr:language="it"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="quiz"/>
               <j:translation_pt jcr:language="pt"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="questionário"/>
               <pagecontent jcr:primaryType="jnt:contentList">
                  <header-1 jcr:primaryType="jnt:area">
                     <header-1 jcr:mixinTypes="jmix:systemNameReadonly"
                               jcr:primaryType="jnt:contentList">
                        <quizheading j:bindedComponent="#/modules/game-4-jcustomer-components-quiz/templates/game4quiz/pagecontent/main-resource-display"
                                     jcr:primaryType="game4nt:quizHeading"/>
                     </header-1>
                  </header-1>
                  <main-resource-display j:mainResourceView="react" jcr:primaryType="jnt:mainResourceDisplay"/>
               </pagecontent>
            </game4quiz>
         </templates>
      </game-4-jcustomer-components-quiz>
   </modules>
</content>
